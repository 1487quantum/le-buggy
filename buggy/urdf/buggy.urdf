<robot name="buggy_2d">
  <material name="orange">
    <color rgba="1.0 0.5 0.2 1" />
  </material>
<material name="white">
    <color rgba="1 1 1 1" />
  </material>
  <material name="dk_gray">
    <color rgba="0.2 0.2 0.2 1" />
  </material>
<material name="lt_gray">
    <color rgba="0.5 0.5 0.5 1" />
  </material>
  <material name="green">
    <color rgba="0.2 0.4 0.2 1" />
  </material>

  <link name="imu_link">
    <visual>
      <geometry>
        <box size="0.057 0.042 0.023" />
      </geometry>
      <material name="orange" />
    </visual>
  </link>

  <joint name="imu_link_joint" type="fixed">
    <parent link="base_link" />
    <child link="imu_link" />
      <origin rpy="0 0 0" xyz="0.39 -0.47 0.05" />
  </joint>

  <link name="laser">
    <visual>
      <geometry>
        <cylinder length="0.09" radius="0.07" />
      </geometry>
      <material name="dk_gray" />
    </visual>
  </link>


    <joint name="laser_joint" type="fixed">
      <parent link="base_link" />
      <child link="laser" />
      <origin xyz="-0.31 0 1.715" />
    </joint>

 <link name="beam">
    <visual>
      <geometry>
        <box size="0.05 0.05 1.31"/>
      </geometry>
      <material name="lt_gray" />
    </visual>
  </link>
  <joint name="beam_link_joint" type="fixed">
      <parent link="seat" />
      <child link="beam" />
      <origin xyz="0 0 0.85435" />
    </joint>

 <link name="seat">
    <visual>
      <geometry>
        <box size="1.1 1.12 0.39"/>
      </geometry>
      <material name="lt_gray" />
    </visual>
  </link>
  <joint name="seat_link_joint" type="fixed">
      <parent link="base_link" />
      <child link="seat" />
      <origin xyz="-0.35 0 0.24" />
    </joint>

  <link name="front">
     <visual>
       <geometry>
         <box size="0.6 1.12 0.39"/>
       </geometry>
       <material name="lt_gray" />
     </visual>
   </link>
   <joint name="front_link_joint" type="fixed">
       <parent link="base_link" />
       <child link="front" />
       <origin xyz="0.93 0 0.24" />
     </joint>

<link name="base_link">
<visual>
      <origin xyz="0 0 0" />
      <geometry>
        <box size="2.43 1.12 0.09" />
      </geometry>
      <material name="white" />
    </visual>
  </link>


<link name="left_front_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.22" radius="0.21"/>
      </geometry>
      <material name="black"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
  </link>
  <joint name="left_front_wheel" type="fixed">
    <parent link="base_link"/>
    <child link="left_front_wheel"/>
    <origin rpy="0 0 0" xyz="1.02 0.43 -0.05"/>
  </joint>

<link name="right_front_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.22" radius="0.21"/>
      </geometry>
      <material name="black"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
  </link>
  <joint name="right_front_wheel" type="fixed">
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="right_front_wheel"/>
    <origin rpy="0 0 0" xyz="1.02 -0.43 -0.05"/>
  </joint>

<link name="left_back_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.22" radius="0.21"/>
      </geometry>
      <material name="black"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
  </link>
  <joint name="left_back_wheel" type="fixed">
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="left_back_wheel"/>
    <origin rpy="0 0 0" xyz="-0.63 0.43 -0.05"/>
  </joint>

<link name="right_back_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.22" radius="0.21"/>
      </geometry>
      <material name="black"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
  </link>
  <joint name="right_back_wheel" type="fixed">
    <parent link="base_link"/>
    <child link="right_back_wheel"/>
    <origin rpy="0 0 0" xyz="-0.63 -0.43 -0.05"/>
  </joint>

</robot>
